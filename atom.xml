---
layout: nil
reference: jweslley.github.com
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 <title>Jonhnny Weslley</title>
 <link href="http://{{ page.reference }}atom.xml" rel="self"/>
 <link href="http://{{ page.reference }}"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://{{ page.reference }}</id>
 <author>
   <name>Jonhnny Weslley</name>
   <email>jonhnnyweslley@gmail.com</email>
 </author>
 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://{{ page.reference }}{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://{{ page.reference }}{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
   <author>
     <name>Jonhnny Weslley</name>
     <uri>http://{{ page.reference }}about/</uri>
   </author>
 </entry>
 {% endfor %}
</feed>

